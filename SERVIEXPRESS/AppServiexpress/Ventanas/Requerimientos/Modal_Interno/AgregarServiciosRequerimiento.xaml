        xmlns:local="clr-namespace:AppServiexpress.Ventanas.Requerimientos.Modal_Interno"
        mc:Ignorable="d"
        Title="Agregar Servicios" Height="363.5" Width="589.59" WindowStartupLocation="CenterScreen" Icon="/AppServiexpress;component/Contenidos/Imagenes/Serviexpress_icono2.png" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
    <Grid Background="#FFFB9F9F">
