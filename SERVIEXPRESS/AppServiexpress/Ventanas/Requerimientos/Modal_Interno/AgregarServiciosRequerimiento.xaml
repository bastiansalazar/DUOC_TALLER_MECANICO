        <TextBox x:Name="txtNombreSucursal" HorizontalAlignment="Left" Height="26"  Width="431" Margin="115,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" VerticalContentAlignment="Center" CharacterCasing="Upper" MaxLength="48" IsReadOnly="True"/>
        <ComboBox x:Name="cbxSerivioc" HorizontalAlignment="Left" Margin="115,149,0,0" VerticalAlignment="Top" Height="26"  Width="431" SelectionChanged="cbxSerivioc_SelectionChanged" />
        <Label Content="Servicio" HorizontalAlignment="Left" Margin="38,149,0,0" VerticalAlignment="Top" Width="67" RenderTransformOrigin="0.509,0.577"/>
        <Label Content="Costo" HorizontalAlignment="Left" Margin="38,180,0,0" VerticalAlignment="Top" Width="86" RenderTransformOrigin="0.806,0.462"/>
        <TextBox x:Name="txtValor" HorizontalAlignment="Left" Height="26"  Width="119" Margin="115,180,0,0" TextWrapping="Wrap" VerticalAlignment="Top" VerticalContentAlignment="Center" CharacterCasing="Upper" MaxLength="48" IsReadOnly="True"/>
